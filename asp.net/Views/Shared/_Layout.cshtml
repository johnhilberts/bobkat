<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>BobKat </title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">
        <link href="@Url.Content("~/Content/css/normalize.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/css/main.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Content/js/vendor/modernizr-2.6.1.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/js/vendor/jquery-1.8.0.min.js")" type="text/javascript"></script>
        <script>            window.jQuery || document.write('<script src="@Url.Content("~/Content/js/vendor/jquery-1.8.0.min.js")"><\/script>')</script>
        @if (IsSectionDefined("AddToHead"))
        {
            @RenderSection("AddToHead", required: false)
        }

    </head>
    <body id="@ViewBag.PageTitle">
        <div id="mainContainer">
            <header>@Html.ActionLink(" ", "Home")</header>
            <div class="content">
                @RenderBody()
            </div>
            <footer>
                <div class="clearfix">
                    <div class="hr"></div>
                    <a href="@Url.Content("~/")" id="footerLink">www.bobkat.com.au</a>
                    <div class="hr"></div>
                </div>
            </footer>
            <nav>
                <ul class="clearfix">
                    <li>@Html.ActionLink("THE STORY", "TheStory", "Home")</li>
                    <li>@Html.ActionLink("THE DAY", "TheDay", "Home")</li>
                    <li>@Html.ActionLink("THE VENUE", "TheVenue", "Home")</li>
                    <li class="noborder">@Html.ActionLink("THE PHOTOS", "Index", "Photos")</li>
                </ul>
                <img id="footerFlowers" src="@Url.Content("~/Content/img/flowers.png")" alt="" />
            </nav>
        </div>
    </body>
</html>