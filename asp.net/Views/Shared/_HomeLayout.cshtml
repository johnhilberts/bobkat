<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>BobKat </title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">
        <link rel="stylesheet" href="@Url.Content("~/Content/css/normalize.css")">
        <link rel="stylesheet" href="@Url.Content("~/Content/css/main.css")">
        <script src="@Url.Content("~/Content/js/vendor/modernizr-2.6.1.min.js")"></script>
    </head>
    <body id="home">
        <div id="mainContainer">
            <header>@Html.ActionLink(" ", "Home")</header>
            <div class="content">
                @this.RenderBody()
            </div>
            <nav>
                <ul class="clearfix">
                    <li>@Html.ActionLink("THE STORY", "TheStory", "Home")</li>
                    <li>@Html.ActionLink("THE DAY", "TheDay", "Home")</li>
                    <li>@Html.ActionLink("THE VENUE", "TheVenue", "Home")</li>
                    <li class="noborder">@Html.ActionLink("THE PHOTOS", "Index", "Photos")</li>
                </ul>
            </nav>
            <footer>
                <div class="clearfix">
                    <div class="hr"></div>
                    <a href="@Url.Content("~/")" id="footerLink">www.bobkat.com.au</a>
                    <div class="hr"></div>
                </div>
                <img id="footerFlowers" src="@Url.Content("~/Content/img/flowers.png")" alt="" />
            </footer>
        </div>


        <script src="@Url.Content("~/Content/js/vendor/jquery-1.8.0.min.js")"></script>
        <script>            window.jQuery || document.write('<script src="@Url.Content("~/Content/js/vendor/jquery-1.8.0.min.js")"><\/script>')</script>
        <script src="@Url.Content("~/Content/js/plugins.js")"></script>
    </body>
</html>

